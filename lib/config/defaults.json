{
  "server": {
    "host": "localhost",
    "port": 8080,
    "enableHttps": false
  },
  "db": {
    "host": "localhost",
    "port": 27017,
    "dbName": "cardgen",
    "user": "cardgen",
    "password": "$secrets.db.password"
  },
  "cardSvgCache": {
    "maxSize": 100
  },
  "log": {
    "level": "debug"
  },
  "i18n": {
    "availableLocales": ["en"],
    "defaultLocale": "en"
  },
  "eolApiKey": "$secrets.eolApiKey",
  "bugsnag": {
    "enable": false
  }
}
